{
  "name": "recipes",
  "version": "1.0.0",
  "description": "Track recipes and build grocery lists",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "spago run --main Recipes.Backend.Main --purs-args --stash",
    "build": "spago build --purs-args --stash",
    "test": "spago test --purs-args --stash",
    "pack": "npm run build && webpack",
    "watch": "npm run build && webpack -w",
    "clean": "rm -r output/Recipes*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntwilson/recipes.git"
  },
  "author": "ntwilson@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ntwilson/recipes/issues"
  },
  "homepage": "https://github.com/ntwilson/recipes#readme",
  "devDependencies": {
    "purescript": "^0.13.8",
    "purescript-psa": "^0.8.2",
    "spago": "^0.19.1",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
